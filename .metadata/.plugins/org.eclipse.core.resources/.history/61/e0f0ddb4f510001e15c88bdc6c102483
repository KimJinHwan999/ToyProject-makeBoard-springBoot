$(document).ready(function(){
	
	
	Highcharts.chart('container', {
	let date = today.getDate();
	    chart: {
	        type: 'column'
	    },
	    title: {
	        text: '게시판 별 업로드 된 글의 갯수'
	    },
	    xAxis: {
	        categories: ['6/20', '6/21', date, '2018/19', '2017/18']
	    },
	    yAxis: {
	        min: 0,
	        title: {
	            text: 'Assists'
	        }
	    },
	    tooltip: {
	        pointFormat: '<span style="color:{series.color}">{series.name}</span>: <b>{point.y}</b> ({point.percentage:.0f}%)<br/>',
	        shared: true
	    },
	    plotOptions: {
	        column: {
	            stacking: 'percent'
	        }
	    },
	    series: [{
	        name: 'Kevin De Bruyne',
	        data: [4, 4, 2, 4, 4]
	    }, {
	        name: 'Joshua Kimmich',
	        data: [0, 4, 3, 2, 3]
	    }, {
	        name: 'Sadio Mané',
	        data: [1, 2, 2, 1, 2]
	    }]
	});

})