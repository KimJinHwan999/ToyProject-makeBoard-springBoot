$(document).ready(function(){
	
	// JSON.parse()
	
	Highcharts.chart('container', {
	    chart: {
	        type: 'column'
	    },
	    title: {
	        text: '게시판 별 업로드 된 글의 갯수'
	    },
	    xAxis: {
	        categories: ['6/21', '6/22', '6/23', '6/24', '6/25']
	    },
	    yAxis: {
	        min: 0,
	        title: {
	            text: '글의 갯수'
	        }
	    },
	    tooltip: {
	        pointFormat: '<span style="color:{series.color}">{series.name}</span>: <b>{point.y}</b> ({point.percentage:.0f}%)<br/>',
	        shared: true
	    },
	    plotOptions: {
	        column: {
	            stacking: 'percent'
	        }
	    },
	    series: [{
	        name: '유머 게시판',
	        data: [4, 4, 2, 4, 4]
	    }, {
	        name: '스포츠 게시판',
	        data: [0, 4, 3, 2, 3]
	    }, {
	        name: '유머 게시판',
	        data: [1, 2, 2, 1, 2]
	    }]
	});

})